{
	"expo": {
		"name": "Menuteca",
		"slug": "menuteca-expo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon-store.png",
		"scheme": "menutecaapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"associatedDomains": [
			"applinks:menutecaapp.com",
			"applinks:www.menutecaapp.com"
		],
		"ios": {
			"supportsTablet": true,
			"usesAppleSignIn": true,
			"bundleIdentifier": "com.menutecaapp.mobile",
			"entitlements": {
				"com.apple.developer.applesignin": ["Default", "Default"],
				"com.apple.developer.associated-domains": [
					"applinks:menutecaapp.com",
					"applinks:www.menutecaapp.com"
				]
			},
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			},
			"appleTeamId": "4WWBHK245Y"
		},
		"android": {
			"icon": "./assets/images/icon-store.png",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon_white_splash_screen.png",
				"backgroundColor": "#48280E"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.menutecaapp.mobile",
			"intentFilters": [
				{
					"action": "VIEW",
					"autoVerify": true,
					"data": [
						{
							"scheme": "https",
							"host": "menutecaapp.com",
							"pathPrefix": "/restaurant"
						},
						{
							"scheme": "https",
							"host": "www.menutecaapp.com",
							"pathPrefix": "/restaurant"
						},
						{
							"scheme": "menutecaapp"
						}
					],
					"category": ["BROWSABLE", "DEFAULT"]
				}
			]
		},
		"web": {
			"bundler": "metro",
			"output": "single",
			"favicon": "./assets/images/icon-store.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#48280E",
					"image": "./assets/images/icon_white_splash_screen.png",
					"imageWidth": 100,
					"resizeMode": "contain",
					"ios": {
						"imageWidth": 200,
						"imageHeight": 200
					},
					"dark": {
						"image": "./assets/images/icon_white_splash_screen.png",
						"backgroundColor": "#48280E"
					}
				}
			],
			"expo-localization",
			"expo-web-browser",
			[
				"@react-native-google-signin/google-signin",
				{
					"iosUrlScheme": "com.googleusercontent.apps.786950108695-jrj6enqmrqv6mdmqm9kieflsjfoaksdh"
				}
			],
			"expo-apple-authentication"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "823c5540-56c9-4294-95f8-f382ed5b49c4"
			}
		},
		"owner": "monum"
	}
}
