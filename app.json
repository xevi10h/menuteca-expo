{
	"expo": {
		"name": "menuteca-expo",
		"slug": "menuteca-expo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon_white_splash_screen.png",
		"scheme": "menutecaexpo",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.menutecaapp.mobile"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon_white_splash_screen.png",
				"backgroundColor": "#48280E"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.menutecaapp.mobile"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/icon_white_splash_screen.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#48280E",
					"image": "./assets/images/icon_white_splash_screen.png",
					"imageWidth": 100,
					"resizeMode": "contain",
					"ios": {
						"imageWidth": 200,
						"imageHeight": 200
					},
					"dark": {
						"image": "./assets/images/icon_white_splash_screen.png",
						"backgroundColor": "#48280E"
					}
				}
			],
			"expo-localization",
			"expo-web-browser",
			[
				"@react-native-google-signin/google-signin",
				{
					"iosUrlScheme": "com.googleusercontent.apps.786950108695-1ij1p3412fgbvsl6c5jk6ql42evpstaa"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "823c5540-56c9-4294-95f8-f382ed5b49c4"
			}
		},
		"owner": "monum"
	}
}
